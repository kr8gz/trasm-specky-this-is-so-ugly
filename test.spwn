import "./src"

// normal stuff
$.assert(@trasm::new("").lexed == [[]])
$.assert(@trasm::new("\n").lexed == [[],[]])
$.assert(@trasm::new("operator arg1, arg2").lexed == [["operator", "arg1", ",", "arg2"]])
$.assert(@trasm::new("sus:").lexed == [["sus", ":"]])
$.assert(@trasm::new("op sus:").lexed == [["op", "sus:"]])
$.assert(@trasm::new(".amongus").lexed == [[".", "amongus"]])
$.assert(@trasm::new("op .amongus").lexed == [["op", ".amongus"]])
$.assert(@trasm::new("; this is a comment").lexed == [[]])
$.assert(@trasm::new("hello guys ; this is a comment").lexed == [["hello", "guys"]])
// errors
$.assert(@trasm::new("sussy 'uncompleted string").lexed == [null])

